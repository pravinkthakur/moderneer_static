      "2": { "name": "Developing", "min": 2.0, "max": 2.9, "color": "#F59E0B" },
      "3": { "name": "Defined", "min": 3.0, "max": 3.9, "color": "#10B981" },
      "4": { "name": "Managed", "min": 4.0, "max": 4.4, "color": "#3B82F6" },
      "5": { "name": "Optimizing", "min": 4.5, "max": 5.0, "color": "#8B5CF6" }
    }
  }
}